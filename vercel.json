{
    "version": 2,
    "builds": [
      {
        "src": "public", 
        "use": "@vercel/static",
        "config": {}
      },
      {
        "src": "vercel-build.sh", 
        "use": "@vercel/ruby",
        "config": {
          "ruby-version": "3.2.2" 
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "env": {
      "RUBY_ENV": "production" 
    }
  }  